<compass-core-mapping package="edu.ucla.mbi.dip">

    <class name="Organism" alias="organism" root="true">

        <id name="id"/>
           
        <property name="taxId">
            <meta-data>taxid</meta-data>
        </property>

        <property name="commonName">
            <meta-data>taxcomm</meta-data>
            <meta-data>name</meta-data>
        </property>
        <property name="name">
            <meta-data>taxname</meta-data>
            <meta-data>name</meta-data>
        </property>

    </class>

</compass-core-mapping>
